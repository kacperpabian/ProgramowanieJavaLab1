<?import javafx.scene.text.Font?>

<GridPane alignment="center" hgap="10" prefHeight="300.0" prefWidth="300.0" vgap="10" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <Button fx:id="buttonGenerate" mnemonicParsing="false" onAction="#generate" prefHeight="39.0" prefWidth="154.0" text="Proceed to menu" GridPane.rowIndex="1">
         <font>
            <Font size="15.0" />
         </font></Button>
      <Label prefHeight="71.0" prefWidth="154.0" text="Welcome" textAlignment="CENTER">
         <font>
            <Font size="36.0" />
         </font></Label>
   </children>
</GridPane>
